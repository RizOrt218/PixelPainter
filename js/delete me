function Swatch() {
  var colorArr = ['#8debff', '#59e2ff', '#2bdaff', '#00d2ff', '#15c2e7', '#00d2ff'];

  for ( var i = 0; i < colorArr.length; i++) {
    new ColorBtn( colorArr[i] );
  }
}

function ColorBtn( color, height, width ) {
  this.height = '25px';
  this.width = '25px';
  this.color = color;
  console.log( color );
  // create HTMLEntity

}